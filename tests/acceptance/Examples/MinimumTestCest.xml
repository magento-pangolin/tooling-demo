        <dependency type="AcceptanceTester">I</dependency>

        <step actor="I" function="amOnUrl"         parameter="'http://127.0.0.1:32770/admin/'" name="amOnPage"/>
        <step actor="I" function="fillField"       selector="#username" parameter="admin"      name="fillField"/>
