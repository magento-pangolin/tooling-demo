<?xml version="1.0" encoding="UTF-8"?>
<!-- Test XML Example -->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../_support/Generators/testXML.xsd">
    <annotations>
        <env>chrome</env>
        <group>example</group>
    </annotations>

    <test name="MinimumFieldsTest">
        <annotations>
            <return>void</return>
